{
	"name": "alexanderdortgmbh/pslzme",
	"description": "A contao bundle that handles custom logic for generated links by pslzme.com",
	"type": "contao-bundle",
	"homepage": "https://www.pslzme.com",
	"autoload": {
		"psr-4": {
			"Robindort\\PslzmeLinks\\": "src/"
		}
	},
	"authors": [
		{
			"name": "Alexander Dort GmbH",
			"homepage": "https://www.pslzme.com"
		}
	],
	"support": {
		"issues": "https://github.com/RobinDort/pslzme/issues"
	},
	"require": {
		"contao/core-bundle": "^4.13",
		"php": ">=8.0"
	},
	"require-dev": {
		"contao/manager-plugin": "^2.0"
	},
	"conflict": {
		"contao/manager-plugin": "<2.0 || >=3.0"
	},
	"extra": {
		"contao-manager-plugin": "RobinDort\\PslzmeLinks\\ContaoManager\\Plugin",
		"contao": {
			"sources": {
				"templates/": "src/Resources/contao/templates"
			},
			"resources": [
				"src/Resources"
			]
		}
	}
}
